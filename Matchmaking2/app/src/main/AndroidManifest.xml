<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Matchmaking"
        tools:targetApi="31">
        <activity
            android:name=".Perfil"
            android:exported="false"
            android:label="@string/title_activity_perfil"
            android:theme="@style/Theme.Matchmaking" />
        <activity
            android:name=".CadastroTags"
            android:exported="false"
            android:label="@string/title_activity_cadastro_tags"
            android:theme="@style/Theme.Matchmaking" />
        <activity
            android:name=".CadastroFotos"
            android:exported="false"
            android:label="@string/title_activity_cadastro_fotos"
            android:theme="@style/Theme.Matchmaking" />
        <activity
            android:name=".CadastroCore"
            android:exported="false"
            android:label="@string/title_activity_cadastro_core"
            android:theme="@style/Theme.Matchmaking" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Matchmaking">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>